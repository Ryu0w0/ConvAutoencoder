{
  "use_cnn": true,
  "use_cae": true,
  "only_train_cae_until": 20,
  "cnn": {
    "use_mixed_input": true,
    "input_resolution": 16,
    "block_sizes": [[32, 32], [32, 64], [64, 128]],
    "opt": {
      "lr": 1e-05,
      "betas": [
        0.0,
        0.999
      ]
    }
  },
  "cae": {
    "enc_block_sizes": [[3, 16]],
    "dec_block_sizes": [[16, 3]],
    "opt": {
      "lr": 1e-05,
      "betas": [
        0.0,
        0.999
      ]
    }
  }
}